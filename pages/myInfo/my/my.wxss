/**index.wxss**/

.my-head {
    padding: 45rpx 0 65rpx;
    text-align: center;
    background: #fff;
}
.my-head .img {
    width: 140rpx;
    height: 140rpx;
    margin: auto;
    overflow: hidden;
    border-radius: 50%;
}
.my-head .img image {
    width: 100%;
    height: 100%;
}
.my-head .tel {
    margin-top: 30rpx;
    font-size: 38rpx;
}
.my-main{
    padding: 20rpx 0;
    margin-top: 20rpx;
    background-color: #fff;
}
.weui-grids {
    background: #fff;
    border: 0;
}
.weui-grid {
    border: 0;
}
.weui-grid_active{
    background: #fff;
}
.my-main .icon{
    display: block;
    color: #2485f0;
    font-size: 68rpx;
    text-align: center;
    height: 66rpx;
}
.my-main .label{
    display: block;
    text-align: center;
    margin-top: 20rpx;
    font-size: 32rpx;
}
.login-out {
    margin-top: 10rpx;
    padding: 30rpx;
    text-align: center;
    font-size: 38rpx;
    background: #fff;
}