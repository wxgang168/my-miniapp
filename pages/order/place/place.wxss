/**place.wxss**/

.item-btn{
    margin-left: 20rpx;
}
.item-btn .iconfont{
    font-size: 47rpx;
}
.tms-checker-inline{
    font-size: 26rpx;
    margin-right: -30rpx;
    text-align: right;
}
.tms-checker-inline .tms-checker__item{
    display: inline-block;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin: 20rpx 30rpx 0 0;
    text-align: center;
    box-sizing: border-box;
    border: 1rpx solid #d9d9d9;
    border-radius: 10rpx;
}
.tms-checker{
    font-size: 26rpx;
    padding-bottom: 20rpx;
}
.tms-checker .tms-checker__item{
    float: left;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-top: 20rpx;
    margin-right: 30rpx;
    text-align: center;
    box-sizing: border-box;
    border: 1rpx solid #d9d9d9;
    border-radius: 10rpx;
}
.tms-checker .tms-checker__item:nth-child(4n){
    margin-right: 0;
}
.tms-checker__item.checked{
    color: #2485f0;
    border: 1rpx solid #2485f0;
}